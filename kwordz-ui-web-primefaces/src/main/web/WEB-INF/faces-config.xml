<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>kwordzManagementView</from-outcome>
			<to-view-id>/private/__management__/kwordz.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>libraryView</from-outcome>
			<to-view-id>/public/library.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>singerConsultView</from-outcome>
			<to-view-id>/public/singer/consult.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>singerListView</from-outcome>
			<to-view-id>/public/singer/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>singerEditView</from-outcome>
			<to-view-id>/private/singer/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>albumConsultView</from-outcome>
			<to-view-id>/public/album/consult.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>albumListView</from-outcome>
			<to-view-id>/public/album/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>songConsultView</from-outcome>
			<to-view-id>/public/song/consult.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>songListView</from-outcome>
			<to-view-id>/public/song/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>songEditView</from-outcome>
			<to-view-id>/private/song/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>
	
   
</faces-config>
