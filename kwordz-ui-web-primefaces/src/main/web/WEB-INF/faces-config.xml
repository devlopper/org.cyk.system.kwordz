<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>singerView</from-outcome>
			<to-view-id>/public/singer/view.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>albumView</from-outcome>
			<to-view-id>/public/album/view.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>songView</from-outcome>
			<to-view-id>/public/song/view.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>
	
   
</faces-config>
